  This is a "Solution Style" tsconfig.json file, and is used by editors and TypeScriptâ€™s language server to improve development experience.
  It is not intended to be used to perform a compilation.

  To learn more about this file see: https://angular.io/config/solution-tsconfig.
*/
{
    "experimentalDecorators": true,
    "files": [],
    "references": [
        {
            "path": "./src/tsconfig.app.json"
        },
        {
            "path": "./src/tsconfig.spec.json"
        },
        {
            "path": "./e2e/tsconfig.e2e.json"
        }
    ],
    "baseUrl": "./",
    "paths": {
        "@services/*": ["src/app/services/*"],
        "@pipes/*": ["src/app/pipes/*"],
        "@components/*": ["src/app/components/*"],
        "@guards/*": ["src/app/guards/*"],
        "@models*": ["src/app/models/"],
        "@environments/*": ["src/environments/*"],
        "@scss/*": ["src/assets/scss/*"],
        "@css/*": ["src/assets/css/*"],
        "@images/*": ["src/assets/images/*"],
        "@utils*": ["src/app/core/utils/"],
        "@sharedscss/*": ["../shared-assets/scss/*"],
        "@sharedcss/*": ["../shared-assets/css/*"],
        "@sharedmodule/*": ["../shared-module/*"],
        "@javascript/*": ["src/assets/js/*"],
        "@sharedjavascript/*": ["../shared-assets/js/*"],
        "@sharedimages/*": ["../shared-assets/images/*"],
        "@fonts/*": ["../shared-assets/fonts/*"],
        "@icon/*": ["../shared-assets/icon/*"],
        "@locales/*": ["../shared-assets/locales/*"],
        "@pages/*": ["../shared-assets/pages/*"],
        "@pcassets/*": ["../shared-assets/pcassets/*"],
        "@packages/*": ["../shared-assets/packages/*"]
    }
}
